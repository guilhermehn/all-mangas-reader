{
  "name": "AMR",
  "version": "1.5.6",
  "description": "AllMangaReader Rebirth",
  "main": "index.js",
  "devDependencies": {
    "jshint-stylish": "^1.0.0",
    "jsmin": "^1.0.1",
    "jsonlint": "1.6.x",
    "jsxhint": "^0.9.0",
    "react-tools": "^0.12.2",
    "uglify-js": "^2.4.16"
  },
  "scripts": {
    "postinstall": "mkdir js/components/dist",
    "test": "jsonlint manifest.json _locales/*/*.json",
    "jsx": "jsx js/components/src js/components/build",
    "jsx:lint": "jsxhint -c .jshintrc_build js/components/src/*.js --reporter=./node_modules/jshint-stylish/stylish.js",
    "jsx:watch": "npm run jsx -- --watch",
    "concat": "cat js/components/build/Base.js js/components/build/*Component.js > js/components/dist/Components.js",
    "build": "npm run jsx:lint && npm run jsx && npm run concat"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/AllMangasReader-dev/AMR.git"
  },
  "author": "",
  "license": "GLP v3",
  "bugs": {
    "url": "https://github.com/AllMangasReader-dev/AMR/issues"
  },
  "homepage": "https://github.com/AllMangasReader-dev/AMR"
}
