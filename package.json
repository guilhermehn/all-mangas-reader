{
  "name": "manga-reader",
  "version": "0.0.0",
  "description": "AllMangaReader Rebirth",
  "main": "index.js",
  "scripts": {
    "js": "browserify . | uglifyjs -cm > ./build/main.js",
    "js:watch": "watchify -v -d ./src/App.js -o ./build/main.js",
    "css": "stylus ./styl/main.styl -p | myth -c > ./build/main.css",
    "css:watch": "gulp css:watch",
    "build": "npm run css && npm run js",
    "postinstall": "npm run build"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babelify": "^6.3.0",
    "eslint": "^1.6.0",
    "eslint-plugin-react": "^3.5.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.2",
    "gulp-concat": "^2.6.0",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^0.10.0",
    "gulp-myth": "^1.0.3",
    "gulp-plumber": "^1.0.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.1.0",
    "myth": "^1.5.0",
    "node-notifier": "^4.3.1",
    "stylus": "^0.52.4",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "async": "^1.4.2",
    "cheerio": "^0.19.0",
    "classnames": "^2.1.5",
    "flux": "^2.1.1",
    "jquery": "^2.1.4",
    "keymirror": "^0.1.1",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "superagent": "^1.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/AllMangasReader-dev/AMR.git"
  },
  "author": "Guilherme Nagatomo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AllMangasReader-dev/AMR/issues"
  },
  "homepage": "https://github.com/AllMangasReader-dev/AMR",
  "keywords": [
    "manga",
    "reader",
    "chrome",
    "extension"
  ]
}
